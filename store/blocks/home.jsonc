},
"image#slider2": {
    "props": {
        "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/du3k6akoks0u3xe322aypnaaal8e?response-content-disposition=inline%3B%20filename%3D%22W6S9NXIJ40I86DJI17S40123EYNM3J6CNF24M4Y04GGC9TBSXU.png%22%3B%20filename%2A%3DUTF-8%27%27W6S9NXIJ40I86DJI17S40123EYNM3J6CNF24M4Y04GGC9TBSXU.png&response-content-type=image%2Fpng&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAIRHGNFPZAGTZ7SUA%2F20210821%2Fsa-east-1%2Fs3%2Faws4_request&X-Amz-Date=20210821T204100Z&X-Amz-Expires=300&X-Amz-SignedHeaders=host&X-Amz-Signature=b1c2fd00b5eb85dbb27f1fc133d1903e909c9e485499883b4d18fd2f48bfed9c",
        "maxHeight": 320
    }
},

//Product List
"search-result-layout.customQuery#home": {
    "props": {
        "querySchema": {
            "skusFilter": "FIRST_AVAILABLE",
            "simulationBehavior": "skip"
        }
    }
  }
}
